{
    "name": "fileshortcut",
    "displayName": "fileShortcut",
    "description": "file shortcut extension",
    "version": "0.0.1",
    "publisher": "fileShortcut",
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.copyFileMarkdownLink"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.copyFileMarkdownLink",
                "title": "copyFileMarkdownLink"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "eslint": "^4.11.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "copy-paste": "^1.3.0"
    }
}
